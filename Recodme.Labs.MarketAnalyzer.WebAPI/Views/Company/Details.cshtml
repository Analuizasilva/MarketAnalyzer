            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.StockPrice)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.DebtToEquity)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.DebtToEquity)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.AssetsToLiabilities)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.AssetsToLiabilities)
            </dd>
            <dt class="col-sm-2">
