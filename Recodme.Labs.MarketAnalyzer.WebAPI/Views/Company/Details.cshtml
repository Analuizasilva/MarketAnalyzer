            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.EPSFitness)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.EPSFitness)
            </dd>
            <dt class="col-sm-2">
